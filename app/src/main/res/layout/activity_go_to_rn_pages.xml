<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <Button
        android:id="@+id/go_to_rn_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="前往RN Activity页面"
        />
    <Button
        android:id="@+id/go_to_rn_fragment_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="前往RN Fragment页面"
        />
    <Button
        android:id="@+id/go_to_rn_preLoadBegin_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="点击 预加载RN页面"
        />
    <Button
        android:id="@+id/go_to_rn_preLoad_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="前往RN 预加载页面"
        />
</LinearLayout>